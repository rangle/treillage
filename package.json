{
  "name": "treillage",
  "version": "0.1.0",
  "description": "A weekly newsletter aggregator.",
  "engines": {
    "node": "^8.10.x",
    "yarn": "1.7.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rimraf dist coverage",
    "cover": "./node_modules/.bin/nyc report --reporter=html",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "lint": "eslint .",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava 'src/**/*.test.js' --verbose && npm run cover",
    "test:watch": "ava 'src/**/*.test.js'--watch",
    "start": "NODE_ENV=production webpack-dev-server --colors --no-info --config webpack.config.prod.js"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.55",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/runtime": "^7.0.0-beta.55",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "nodemon": "^1.8.1",
    "nyc": "^12.0.2",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "basscss": "^8.0.4",
    "bluebird": "^3.5.0",
    "clipboard-js": "^0.3.3",
    "connected-react-router": "^4.3.0",
    "immutable": "^3.7.5",
    "leven": "^2.1.0",
    "marked": "^0.4.0",
    "moment": "^2.13.0",
    "prop-types": "^15.5.8",
    "ramda": "^0.19.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.1"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  }
}
