{
  "name": "treillage",
  "version": "0.1.0",
  "description": "A weekly newsletter aggregator.",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "postinstall": "npm run clean; npm run build;",
    "build": "npm run clean; NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "cover": "./node_modules/.bin/nyc report --reporter=html",
    "dev": "NODE_ENV=development nodemon server.js",
    "lint": "eslint .",
    "test": "npm run lint && nyc ava 'src/**/*.test.js' && npm run cover",
    "test:watch": "mocha --opts ./tests/mocha.opts --watch",
    "start": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.1",
    "enzyme": "^2.8.2",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "jsdom": "^7.0.2",
    "nodemon": "^1.8.1",
    "nyc": "^10.2.0",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "dependencies": {
    "babel-loader": "^6.3.2",
    "basscss": "^7.0.4",
    "bluebird": "^3.5.0",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.7.0",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "leven": "^2.1.0",
    "marked": "^0.3.5",
    "moment": "^2.13.0",
    "postcss-loader": "^0.8.0",
    "prop-types": "^15.5.8",
    "ramda": "^0.19.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.4",
    "redux-form": "^3.0.11",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.0.4",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.0",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "semantic-ui-react": "^0.68.0",
    "serve-webpack-client": "^0.0.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  }
}
